{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ValidatorSet",
  "type": "object",
  "required": [
    "size",
    "validators",
    "weights"
  ],
  "properties": {
    "size": {
      "type": "integer",
      "format": "uint",
      "minimum": 0.0
    },
    "validators": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "weights": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false
}
