                "type": "object",
                "required": [
                  "minter"
                ],
                "properties": {
                  "minter": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            },
            "additionalProperties": false
          },
          {
            "type": "object",
            "required": [
              "synthetic_metadata"
            ],
            "properties": {
              "synthetic_metadata": {
                "type": "object",
                "required": [
                  "synthetic"
                ],
                "properties": {
                  "synthetic": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            },
            "additionalProperties": false
          }
        ]
      }
