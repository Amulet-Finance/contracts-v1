{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Metadata",
  "type": "object",
  "required": [
    "decimals",
    "denom",
    "ticker"
  ],
  "properties": {
    "decimals": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "denom": {
      "type": "string"
    },
    "ticker": {
      "type": "string"
    }
  },
  "additionalProperties": false
}
